mvn install:install-file \
  -Dfile=jar/cdk-1.3.1.git.jar \
  -DgroupId=org.openscience.cdk \
  -DartifactId=cdk \
  -Dversion=1.3.1 \
  -Dpackaging=jar \
  -DgeneratePom=true

mvn install:install-file \
  -Dfile=jar/cdk-smsd.jar \
  -DgroupId=org.openscience.cdk \
  -DartifactId=cdk-smsd \
  -Dversion=x.x.x \
  -Dpackaging=jar \
  -DgeneratePom=true
