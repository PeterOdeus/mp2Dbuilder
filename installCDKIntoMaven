mvn install:install-file \
  -Dfile=../jchempaint-primary/dist/jar/cdk-core.jar \
  -DgroupId=org.openscience.cdk \
  -DartifactId=cdk-core \
  -Dversion=bioclipse-2.1.x \
  -Dpackaging=jar \
  -DgeneratePom=true

